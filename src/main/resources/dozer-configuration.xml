<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="
          http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
          
	<mapping>
		<class-a>com.helloworldio.front.model.domain.HWUserImpl</class-a>
		<class-b>com.helloworldio.front.web.domain.SignupUser</class-b>
	</mapping>
	<mapping>
		<class-a>com.helloworldio.front.model.domain.HWProfileUserImpl</class-a>
		<class-b>com.helloworldio.front.web.domain.SignupUser</class-b>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>userId</a>
			<b>email</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.helloworldio.front.model.domain.HWProfileUserImpl</class-a>
		<class-b>com.helloworldio.front.web.domain.ProfileUser</class-b>
		<field>
  			<a>birthdate</a>
  			<b date-format="MM/dd/yyyy">birthdate</b>
		</field>
	</mapping>
</mappings> 